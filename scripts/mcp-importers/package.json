{
  "name": "mcp-importers",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Internal scripts for fetching MCP servers from registries",
  "scripts": {
    "fetch:all": "node fetch-from-registries.js --registry all",
    "fetch:github": "node fetch-from-registries.js --registry github",
    "fetch:smithery": "node fetch-from-registries.js --registry smithery",
    "fetch:docker": "node fetch-from-registries.js --registry docker",
    "test": "node fetch-from-registries.js --registry github --limit 2 --dry-run"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "dotenv": "^16.0.0"
  }
}